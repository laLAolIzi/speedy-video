
{

  "manifest_version": 3,
  "name": "Video Speed Controller",
  "version": "1.0",

  "action": {
    "default_popup": "popup copy.html"
  },

  "permissions": ["scripting",  "activeTab", "tabs"],


  "host_permissions": [],

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [{
    "js": ["content.js"],

    "matches": ["*://*/*"]
  }]

}
